{
	"extractor":
	{
		"base-directory": "./",
		"cookies-update": true,

		"retries": 4,
		"timeout": 30.0,
		"verify": true,
		"fallback": true,

		"path-restrict": "auto",
		"path-replace": "_",
		"path-remove": "\\u0000-\\u001f\\u007f",
		"path-strip": "auto",
		"path-extended": true,

		"extension-map": {
			"jpeg": "jpg",
			"jpe" : "jpg",
			"jfif": "jpg",
			"jif" : "jpg",
			"jfi" : "jpg"
		},
		"filename":"{date:%Y-%m-%d} {id}.{extension}",
		"directory":["{date:%Y-%m-%d} {title} by {username}"],
		"facebook":{
			"cookies": "$HOME/.cache/cookies.txt"
		},
		"last-line-with-no-comma":true
	}
}
